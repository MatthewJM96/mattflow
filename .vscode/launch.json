    "version": "0.2.0",
    "configurations": [
        {
            "name": "GCC x64 Debug",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/GCC-x64-Debug/mattflow_cli",
            "args": [
                "--plot-ast",
                "--dry-run",
                "--profile",
                "--interactive"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/test",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb"
        },
