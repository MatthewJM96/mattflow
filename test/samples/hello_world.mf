func main () -> int {
    print("Hello, world!")

    for x in [0, 1, 2] print(x)

    for y in [3, 4, 5] {
        z = y + 1
        print(z)
    }

    for y in [3, 4, 5], z in [1, 2] {
        w = y + z
        print(w)
    }

    for y in [3, 4, 5], z in [1 .. 4] where z != 2 {
        w = y + z
        print(w)
    }

    0
}
